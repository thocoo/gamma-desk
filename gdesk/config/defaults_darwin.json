{
  "path_log": "~/.gamma-desk-log",
  "max_log_paths": 10,
  "path_config_files": ["~/.config/gamma-desk/gdconf.json", "gdconf.json"],
  "save_config_file": "gdconf.json",
  "init_file": null,
  "init_command": {"cmd": null, "args": []},
  "zmq_watcher_min_port": 5550,
  "zmq_watcher_max_port": 5560,
  "zmq_queue_min_port": 5560,
  "zmq_queue_max_port": 5600,
  "watcher": false,
  "watcher_port": 5998,
  "path_imageio_freeimage_lib": null,
  "solo": false,
  "panic": false,
  "scroll_area": false,
  "hide_solo_panel": true,
  "debug": {
    "skip_restore_perspective": false,
    "skip_main_stdout_redirect": false,
    "skip_main_stderr_redirect": false,
    "place select button": false,
    "list_packages": true
  },
  "console": {
    "fontsize": 8,
    "wrap": false,
    "maxblockcount": 20000,
    "completer": "key",
    "max_complete": 2000,
    "redbull": 300
  },
  "image": {
    "threads": 8,
    "numba": true,
    "render_detail_hq": false,
    "render_detail_smooth": true,
	"bind_zoom_absolute": true,
    "queue_array_shared_mem": false,
    "qimg_shared_mem": false,
    "history_size": 500e6
  },
  "levels": {
    "numba": true,
    "relative": false,
    "hist_size_policy": "bins",
    "bins": 64,
    "step": 4
  },
  "qapp": false,
  "sphinx": false,
  "stdoutmode": "ansi",
  "respath": "$GDESKROOT/resources",
  "qt_api": "pyside2",
  "path_channel_script": "$GDESKROOT/external/scripts/connect_from_canvas.py",
  "path_channel_ini": "$GDESKROOT/external/scripts/connect.ini",
  "logging_level": "INFO",
  "logging_level_console": "WARNING",
  "logging_level_logfile": "DEBUG",
  "system info period": 1000,
  "texteditor": "C:\\Program Files\\Notepad++\\notepad++.exe",
  "dbbrowser": "C:\\Program Files\\DB Browser for SQLite\\DB Browser for SQLite.exe",
  "image background": [
    58,
    110,
    165
  ],
  "roi color": [255,0,0],
  "image color map": "grey",
  "matplotlib": {
    "backend": "module://gdesk.matplotbe"
  },
  "shortcutmenu": {
    "Ctrl+Shift+N": {"image": ["File", "New..."]},
    "Ctrl+Shift+D": {"image": ["File", "Duplicate"]},
    "Ctrl+O": {"image": ["File", "Open Image..."],
               "console": ["File", "Open File"]},
    "Ctrl+W": {"image":   ["File", "Close"],
               "console": ["File", "Close"],
               "levels":  ["File", "Close"],
               "plot":    ["File", "Close"],
               "cmdhist": ["File", "Close"],
               "html":    ["File", "Close"],
               "scriptwiz":  ["File", "Close"]},
    "Ctrl+Shift+[": {"image": ["Edit", "Show Prior Image"]},
    "Ctrl+Shift+]": {"image": ["Edit", "Show Next Image"]},
    "Ctrl+Alt+1": {"image": ["View", "Gain to Sigma", "Gain to Sigma 1"]},
    "Ctrl+Alt+2": {"image": ["View", "Gain to Sigma", "Gain to Sigma 2"]},
    "Ctrl+Alt+3": {"image": ["View", "Gain to Sigma", "Gain to Sigma 3"]},
    "F5": {"image": ["View", "Refresh"],
           "plot": ["View", "Refresh"]},
    "Ctrl++": {"image": ["View", "Zoom In"]},
    "Ctrl+-": {"image": ["View", "Zoom Out"]},
    "Ctrl+0": {"image": ["View", "Zoom", "Zoom Auto"]},
    "Ctrl+1": {"image": ["View", "Default Offset & Gain"]},
    "Ctrl+G": {"image": ["View", "Offset & Gain..."],
	           "plot": ["View", "Grid"]},
    "Ctrl+T": {"plot": ["View", "Tight"]},
    "Ctrl+I": {"plot": ["View", "Interactive"]},
    "Ctrl+B": {"image": ["View", "Black & White..."]},
    "Ctrl+Shift+H": {"image": ["View", "HQ Zoom Out"]},
    "Ctrl+Shift+A": {"image": ["Select", "Select Full Image"]},
    "Ctrl+D": {"image": ["Select", "Deselect"]},
    "Ctrl+J": {"image": ["Select", "Jump to Coordinates"]},
    "Ctrl+Shift+X": {"image": ["Canvas", "Crop on Selection"]},
    "Ctrl+Shift+I": {"image": ["Image", "Invert"]},
    "Esc": {"console": ["Execution", "Sync Break"]},
    "Shift+Esc": {"console": ["Execution", "Async Break"]},
    "ScrollLock": {"console": ["View", "Auto Scroll"]},
    "F10": {"window": ["Cycle Tag Level"]},
    "F11": {"window": ["Full Screen"]},
    "F12": {"window": ["Hide/show Menu & Statusbar"]}
    },
  "shortcuts": {
    "help": "F1",
    "application restart": "Shift+F4",
    "panel preview": "Ctrl+Shift+Tab",
    "window preview": "Shift+Tab",
    "instance info": "Shift+F1",
    "new panel": "Ctrl+N",
    "layout": {
      "prefix": "Ctrl+F",
      "1": "console",
      "2": "image, levels & console",
      "3": "4 images, 1 levels & 1 console"}},
